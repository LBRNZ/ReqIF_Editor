        <ResourceDictionary Source="pack://application:,,,/Fluent;component/Themes/Generic.xaml" >
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="images\Images.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
